{
  "singleQuote": false,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "es5",
  "importOrder": [
    "@/redux/*",
    "@/components/*",
    "@/common/*",
    "@/layouts/*",
    "@/utils/*",
    "@/api/*",
    "@/styles/reset",
    "@/styles/*",
    "@/svg/*",
    "@/images/*"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "endOfLine": "auto"
}
